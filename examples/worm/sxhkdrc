# Restart worm
super + ctrl + r
	worm

# open terminal
super + Return

# program launcher
super + @space
    dmenu_run

# dmenu-script
super + p
  ./mycelaneos

# dmenu-shutdown
super + shift + p
  ./shutdown

# screenshot
#super + x
#    flameshot gui

# Quit worm
ctrl + alt + q
	pkill worm

# Close app
super + q
	wormc close-active-client

# Maximize app
super + f
	wormc maximize-active-client

# Minimize app
super + h
	wormc minimize-active-client

# Tags
super + {_, shift + } {1-9}
	wormc {switch,move-active}-tag {1-9}

# Set master windows
super + m
	wormc master-active

# Float a window
super + shift + space
	wormc float-active
